fix {
  address        = 0.0.0.0
  port           = {{ fix_port }}
  sender-comp-id = genesis
}

order-entry {
  address = 127.0.0.1
  port    = {{ poe_port }}
}

instruments {
{% for product in products %}
  {{ product['id'] }} {
    price-fraction-digits = {{ product['price_fraction_digits'] }}
    size-fraction-digits  = {{ product['size_fraction_digits'] }}
  }
{% endfor %}
}
